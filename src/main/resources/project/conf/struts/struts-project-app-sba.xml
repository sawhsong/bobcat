<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <package extends="default" name="sba" namespace="/sba" strict-method-invocation="false">
        <default-interceptor-ref name="ZebraDefaultStack"/>
        <!--Sba0202 - Organisation Management-->
        <action class="project.app.sba.sba0202.Sba0202Action" method="{1}" name="0202/*">
            <result name="list" type="debugDispatcherResult">/project/app/sba/sba0202/Sba0202List.jsp</result>
            <result name="detail" type="debugDispatcherResult">/project/app/sba/sba0202/Sba0202DetailPop.jsp</result>
            <result name="insert" type="debugDispatcherResult">/project/app/sba/sba0202/Sba0202InsertPop.jsp</result>
            <result name="update" type="debugDispatcherResult">/project/app/sba/sba0202/Sba0202UpdatePop.jsp</result>
        </action>
        <!--Sba0204 - User Management-->
        <action class="project.app.sba.sba0204.Sba0204Action" method="{1}" name="0204/*">
            <result name="list" type="debugDispatcherResult">/project/app/sba/sba0204/Sba0204List.jsp</result>
            <result name="detail" type="debugDispatcherResult">/project/app/sba/sba0204/Sba0204DetailPop.jsp</result>
            <result name="insert" type="debugDispatcherResult">/project/app/sba/sba0204/Sba0204InsertPop.jsp</result>
            <result name="update" type="debugDispatcherResult">/project/app/sba/sba0204/Sba0204UpdatePop.jsp</result>
        </action>
    </package>
</struts>
