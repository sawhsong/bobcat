<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <!--/* Bulletin Board */-->
    <package extends="default" name="bbs" namespace="/bbs" strict-method-invocation="false">
        <default-interceptor-ref name="ZebraDefaultStack"/>
        <!--Bbs0202 - Announcement-->
        <action class="project.app.bbs.bbs0202.Bbs0202Action" method="{1}" name="0202/*">
            <result name="list" type="debugDispatcherResult">/webapp/project/app/bbs/bbs0202/Bbs0202List.jsp</result>
            <result name="detail" type="debugDispatcherResult">/webapp/project/app/bbs/bbs0202/Bbs0202DetailPop.jsp</result>
        </action>
        <!--Bbs0204 - Free Board-->
        <action class="project.app.bbs.bbs0204.Bbs0204Action" method="{1}" name="0204/*">
            <result name="list" type="debugDispatcherResult">/webapp/project/app/bbs/bbs0204/Bbs0204List.jsp</result>
            <result name="detail" type="debugDispatcherResult">/webapp/project/app/bbs/bbs0204/Bbs0204Detail.jsp</result>
            <result name="insert" type="debugDispatcherResult">/webapp/project/app/bbs/bbs0204/Bbs0204Insert.jsp</result>
            <result name="update" type="debugDispatcherResult">/webapp/project/app/bbs/bbs0204/Bbs0204Update.jsp</result>
        </action>
    </package>
</struts>
